{
  "name": "example/slim-meilisearch",
  "description": "Slim + MySQL + Meilisearch example",
  "type": "project",
  "require": {
    "ext-pdo": "*",
    "php": "^8.2",
    "slim/slim": "^4.13",
    "meilisearch/meilisearch-php": "^1.16",
    "guzzlehttp/guzzle": "^7.9",
    "symfony/console": "^7.4",
    "symfony/dotenv": "^7.4",
    "nyholm/psr7": "^1.8",
    "nyholm/psr7-server": "^1.1",
    "monolog/monolog": "^3.10",
    "php-di/php-di": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
